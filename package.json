    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@popperjs/core": "^2.11.8",
    "@sentry/sveltekit": "^7.93.0",
    "@tailwindcss/typography": "^0.5.13",
    "@tinymce/tinymce-svelte": "^2.0.2",
    "@zerodevx/svelte-toast": "^0.9.5",
    "credit-card-type": "^10.0.0",
