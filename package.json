{
	"name": "dsh-fe",
	"version": "1.5.0",
	"private": true,
	"scripts": {
		"start": "PORT=8080 node build/index.js",
		"dev": "vite dev",
		"sitemap": "npx tsx ./scripts/generate-sitemap.ts",
		"prep": "npx svelte-kit prepare && npm run check && npm run lint && npm run format",
		"prebuild-next": "npx workbox-cli generateSW workbox-config.cjs",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@sveltejs/kit": "^1.20.4",
		"@types/lodash": "^4.14.202",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"autoprefixer": "^10.4.16",
		"directory-tree": "^3.5.1",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.30.0",
		"postcss": "^8.4.31",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"svelte": "^4.0.5",
		"svelte-check": "^3.4.3",
		"tailwindcss": "^3.3.5",
		"tslib": "^2.4.1",
		"tsx": "^3.8.0",
		"typescript": "^5.2.2",
		"vite": "^4.5.1"
	},
	"type": "module",
	"dependencies": {
		"@aws-sdk/client-s3": "^3.454.0",
		"@aws-sdk/s3-request-presigner": "^3.454.0",
		"@fortawesome/free-brands-svg-icons": "^6.4.2",
		"@fortawesome/free-solid-svg-icons": "^6.4.2",
		"@sveltejs/adapter-node": "^1.3.1",
		"@zerodevx/svelte-toast": "^0.9.5",
		"lodash": "^4.17.21",
		"node-localstorage": "^3.0.5",
		"svelte-fa": "^3.0.4",
		"uuid": "^9.0.1",
		"workbox": "^0.0.0",
		"xml-formatter": "^3.6.0"
	}
}
