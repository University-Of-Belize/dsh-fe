    "html2canvas": "^1.4.1",
    "insane": "^2.6.2",
    "lodash": "^4.17.21",
    "marked": "^12.0.2",
    "node-localstorage": "^3.0.5",
    "pixi.js": "^8.1.5",
    "svelte-fa": "^4.0.2",
    "tailwind-merge": "^2.2.1",
    "turndown": "^7.2.0",
    "uuid": "^9.0.1",
    "workbox": "^0.0.0",
    "xml-formatter": "^3.6.2"
