{
  "name": "dsh-fe",
  "version": "1.20.6",
  "private": true,
  "scripts": {
    "start": "PORT=8080 node build/index.js",
    "dev": "vite dev",
    "sitemap": "npx tsx ./scripts/generate-sitemap.ts",
    "prep": "npx svelte-kit prepare && npm run check && npm run lint && npm run format",
    "prebuild-next": "npx workbox-cli generateSW workbox-config.cjs",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write ."
  },
  "devDependencies": {
    "@sveltejs/kit": "^2.5.10",
    "@types/lodash": "^4.17.4",
    "@types/turndown": "^5.0.4",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "autoprefixer": "^10.4.19",
    "directory-tree": "^3.5.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.39.0",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.2.3",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "svelte": "^4.2.17",
    "svelte-adapter-bun": "^0.5.2",
    "svelte-check": "^3.7.1",
    "tailwindcss": "^3.4.3",
    "tslib": "^2.6.2",
    "tsx": "^4.11.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.13"
